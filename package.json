{
  "name": "invoice-tracker",
  "version": "0.1.3",
  "author": "Przypis powszechny",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/edukaton/przypis-powszechny",
    "ghbranch": "gh-pages"
  },
  "config": {
    "title": "FakeSpot",
    "mobile": true,
    "baseHref": "/",
    "links": [
      {
        "href": "/favicon.png",
        "rel": "icon"
      },
      "https://fonts.googleapis.com/css?family=Material+Icons|Open+Sans:300,400,700,800",
      "https://cdn.jsdelivr.net/npm/tailwindcss/dist/tailwind.min.css"
    ],
    "lang": "pl-PL",
    "appMountId": "app"
  },
  "browserslist": [
    "defaults"
  ],
  "scripts": {
    "start": "webpack-dev-server --config ./build/webpack.dev.js",
    "deploy": "sh ./build/deploy.sh",
    "clean": "rimraf dist",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --config ./build/webpack.prod.js -p --progress --hide-modules",
    "monitor": "npm run clean && cross-env NODE_ENV=production MONITOR=true webpack --config ./build/webpack.prod.js -p --progress --hide-modules",
    "server": "node ./server/index.js"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-polyfill": "^6.26.0",
    "express": "^4.16.2",
    "history": "^4.7.2",
    "json-server": "^0.12.1",
    "moment": "^2.20.1",
    "offline-plugin": "^4.8.4",
    "postcss-nested": "^3.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.2.0",
    "sass-lint": "^1.12.1",
    "tailwindcss": "^0.4.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.3",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "copy-webpack-plugin": "^4.2.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.29.0",
    "html-webpack-template": "^6.1.0",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "purify-css": "^1.2.5",
    "purifycss-webpack": "^0.7.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.5.1",
    "webpack-monitor": "^1.0.13"
  }
}
